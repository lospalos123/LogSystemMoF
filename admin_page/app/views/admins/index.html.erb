<h1>List of Admin</h1>
<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last name</th>
      <th>Dob</th>
      <th>Email</th>
      <th>Password</th>
      <th>Pass confirmation</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @admins.each do |admin| %>
      <tr>
        <td><%= admin.fname %></td>
        <td><%= admin.lname %></td>
        <td><%= admin.dob %></td>
        <td><%= admin.email %></td>
        <td><%= admin.password %></td>
        <td><%= admin.pass_confirmation %></td>
        <td><%= link_to 'Show', admin %></td>
        <td><%= link_to 'Edit', edit_admin_path(admin) %></td>
        <td><%= link_to 'Destroy', admin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin', new_admin_path %>


 